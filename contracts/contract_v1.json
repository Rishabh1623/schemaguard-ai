{
  "version": 1,
  "created_at": "2025-12-31T00:00:00Z",
  "description": "Initial data contract for SchemaGuard AI platform",
  "schema": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier"
      },
      "timestamp": {
        "type": "integer",
        "description": "Unix timestamp in milliseconds"
      },
      "event_type": {
        "type": "string",
        "description": "Type of event"
      },
      "user_id": {
        "type": "string",
        "description": "User identifier"
      },
      "data": {
        "type": "object",
        "description": "Event payload"
      }
    }
  },
  "required_fields": ["id", "timestamp", "event_type"],
  "optional_fields": ["user_id", "data"],
  "validation_rules": {
    "id": {
      "pattern": "^[a-zA-Z0-9-]+$",
      "min_length": 1,
      "max_length": 100
    },
    "timestamp": {
      "min": 0,
      "max": 9999999999999
    },
    "event_type": {
      "enum": ["user_action", "system_event", "api_call", "data_update"]
    }
  },
  "evolution_policy": "ADDITIVE_ONLY",
  "backward_compatible": true,
  "metadata": {
    "owner": "data-platform-team",
    "domain": "events",
    "classification": "internal"
  }
}
